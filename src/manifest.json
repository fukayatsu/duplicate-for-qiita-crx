{
  "manifest_version": 2,
  "name": "Duplicate for Qiita",
  "version": "0.0.2",
  "permissions": [
    "http://qiita.com/*",
    "https://*.qiita.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://qiita.com/*",
        "https://*.qiita.com/*"
      ],
      "js": [
        "js/jquery-2.1.1.min.js",
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48":  "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "default_locale": "ja"
}
